<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <StartAction>Class</StartAction>
    <WorkingDirectory>bin\</WorkingDirectory>
    <UseRemoteMachine>False</UseRemoteMachine>
    <DebugJvmArguments>-Xincgc -Xmx512M -Xms512M -Dfile.encoding=Cp1252 GradleStart</DebugJvmArguments>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'DebugClient|AnyCPU' ">
    <StartClass>GradleStart</StartClass>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'DebugServer|AnyCPU' ">
    <StartClass>GradleStartServer</StartClass>
  </PropertyGroup>
</Project>